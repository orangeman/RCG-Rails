<h1>New simulation</h1>

<% form_for :simulation, @simulation, :url => { :action => "create" } do |f| %>

  <p>
    <%= f.label :anzahl_autos %><br />
    <%= f.text_field :anzahl %>
  </p>
  <p>
    <%= f.label :status %><br />
    <%= f.text_field :status %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', simulations_path %>
